<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.fmk.huagu.efitness"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET"/>


    <application
        android:name="com.fmk.huagu.efitness.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:resizeableActivity="true"
        android:supportsPictureInPicture="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.fmk.huagu.efitness.Activity.MainActivity">
        </activity>
        <activity android:name="com.fmk.huagu.efitness.Activity.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.fmk.huagu.efitness.Activity.PasswordLoginActivity">
        </activity>
        <activity android:name="com.fmk.huagu.efitness.Activity.CaptchaLoginActivity">
        </activity>
        <activity android:name="com.fmk.huagu.efitness.Activity.RegisterActivity">
        </activity>
        <activity android:name="com.fmk.huagu.efitness.Activity.ForgetPasswordActivity">
        </activity>
    </application>

</manifest>